<!DOCTYPE html>
<html>
<head>
<style>
table, th, td {
  border: 1px solid black;
  border-collapse: collapse;
}
.btn-edit{
  background-color: orange;
    outline: none;
    border-radius: 4px;
    height: 25px;
    width: 60px;
    cursor: pointer;
}
.btn-delete{
  background-color: red;
    outline: none;
    border-radius: 4px;
    height: 25px;
    width: 60px;
    cursor: pointer;
}
</style>
</head>
<body>


<table style="width:100%">
  <tr>
    <th>Email</th>
    <th>First Name</th> 
    <th>Last Name</th>
    <th>Address</th>  
    <th>Action</th>  
  </tr>
  <% for(var i=0; i < dataTable.length; i++) { %>
    <tr>
      <td><%= dataTable[i].email %></td>
      <td><%= dataTable[i].firstName %></td>
      <td><%= dataTable[i].lastName %></td>
      <td><%= dataTable[i].address %></td>
      <td>
        <a href="/edit-crud?id=<%= dataTable[i].id %>" class="btn-edit" type="button">Edit</a>
        <a href="/delete-crud?id=<%= dataTable[i].id %>" class="btn-delete" type="button">Delete</a>
      </td>
    </tr>
  
 <% } %>
  
</table>

</body>
</html>